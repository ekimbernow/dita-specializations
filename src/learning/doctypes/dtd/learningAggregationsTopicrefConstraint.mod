<!ENTITY % keydef                                          "keydef">
<!ENTITY % mapref                                          "mapref">
<!ENTITY % topicgroup                                      "topicgroup">

<!ENTITY % mapgroup-d-topicref
              "%keydef; |
               %mapref; |
               %topicgroup;"
